    "#Creat the columns with init value of zero\n",
    "tdata['Grass'] = 0\n",
    "tdata['Fire'] = 0\n",
    "tdata['Water'] = 0\n",
    "tdata['Bug'] = 0\n",
    "tdata['Normal'] = 0\n",
    "tdata['Electric'] = 0\n",
    "tdata['Dragon'] = 0\n",
    "tdata['Rock'] = 0\n",
    "tdata['Poison'] = 0\n",
    "tdata['Ground'] = 0\n",
    "tdata['Fighting'] = 0\n",
    "tdata['Ghost'] = 0\n",
    "tdata['Fairy'] = 0\n",
    "tdata['Psychic'] = 0\n",
    "tdata['Ice'] = 0\n",
    "\n",
    "\n",
    "\n",
    "#Apply the one hot encoding\n",
    "\n",
    "for i in range(0,len(tdata.primary_strength)):\n",
    "    if tdata.loc[:,('primary_strength')][i] == 'Grass': \n",
    "        tdata.loc[:,'Grass'][i] = 1 \n",
    "        \n",
    "for i in range(0,len(tdata.primary_strength)):\n",
