    "    yt = np.asarray(yt)\n",
    "    \n",
    "    #print(np.linalg.pinv(Xt.transpose()@Xt))\n",
    "    w_fold2 = np.linalg.pinv(XXt.transpose()@XXt+ r_term*np.identity(4) ) @ (XXt.transpose()@yt) \n",
